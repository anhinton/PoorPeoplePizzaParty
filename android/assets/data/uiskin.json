{
  BitmapFont: { button-font: { file: fonts/button.fnt } },
  BitmapFont: { label-font: { file: fonts/label.fnt } },
  BitmapFont: { credits-font: { file: fonts/credits.fnt } },
  Color: {
    green: {
      a: 1,
      b: 0,
      g: 1,
      r: 0
    },
    white: {
      a: 1,
      b: 1,
      g: 1,
      r: 1
    },
    red: {
      a: 1,
      b: 0,
      g: 0,
      r: 1
    },
    black: {
      a: 1,
      b: 0,
      g: 0,
      r: 0
    }
  },
  ButtonStyle: {
    default: {
      down: default-round-down,
      up: default-round
    },
    toggle: {
      down: default-round-down,
      checked: default-round-down,
      up: default-round
    }
  },
  ScrollPaneStyle: {
    credits: { vScroll: default-scroll, hScrollKnob: default-round-large, hScroll: default-scroll, vScrollKnob: default-round-large }
  },
  TextButtonStyle: {
    default: {
      down: default-round-down,
      up: default-round,
      font: button-font,
      fontColor: white
    },
    toggle: {
      down: default-round-down,
      up: default-round,
      checked: default-round-down,
      font: button-font,
      fontColor: white,
      downFontColor: red
    }
  },
  LabelStyle: {
    default: {
      font: label-font,
      fontColor: white
    },
    "credits": {
      "font": credits-font,
      "fontColor": black
    }
  },
  ProgressBarStyle: {
    default-horizontal: { background: default-slider, knob: default-round-large },
    default-vertical: { background: default-slider, knob: default-round-large }
  },
  SliderStyle: {
    default-horizontal: { background: default-slider, knob: default-round-large },
    default-vertical: { background: default-slider, knob: default-round-large }
  }
}
